{
    "apiVersion": "extensions/v1beta1",
    "kind": "Deployment",
    "metadata": {
        "labels": {
            "visualize": "true"
        },
        "name": "api-deployment"
    },
    "spec": {
        "replicas": 12,
        "selector": {
            "matchLabels": {
                "app": "api",
                "visualize": "true"
            }
        },
        "strategy": {
            "type": "RollingUpdate"
        },
        "template": {
            "metadata": {
                "labels": {
                    "app": "api",
                    "visualize": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "image": "api",
                        "imagePullPolicy": "IfNotPresent",
                        "name": "api",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "name": "http",
                                "protocol": "TCP"
                            }
                        ]
                    }
                ]
            }
        }
    }
}
